{
    "dataconnect": {
        "source": "dataconnect"
    },
    "firestore": {
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    },
    "hosting": {
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ]
    },
    "storage": {
        "rules": "storage.rules"
    },
    "database": {
        "rules": "database.rules.json"
    },
    "functions": [{
        "source": "./",
        "codebase": "default",
        "ignore": [
            "node_modules",
            ".git",
            "firebase-debug.log",
            "firebase-debug.*.log",
            "*.local"
        ]
    }]
}